{
	"list.insertMappings": {
		"<a-k>": "normal:k",
		"<a-j>": "normal:j"
	},
	"list.source.files.defaultOptions": [
		"--number-select"
	],
	"list.source.buffers.defaultOptions": [
		"--number-select",
	],
	"list.source.commits.defaultOptions": [
		"--auto-preview"
	],
	"list.source.branches.defaultOptions": [
		"--auto-preview"
	],
	"list.source.symbols.defaultOptions": [
		"--interactive",
	],
	"languageserver": {
		"rust": {
			"enable": false,
			"command": "ra_lsp_server",
			"filetypes": [
				"rust"
			],
			"rootPatterns": [
				"Cargo.toml"
			]
		},
		"go": {
			"enable": true,
			"command": "gopls",
			"filetypes": [
				"go"
			],
			"rootPatterns": [
				"go.mod"
			]
		}
	},
	"suggest.timeout": 5000,
	"translator.toLang": "zh",
	"coc.preferences.currentFunctionSymbolAutoUpdate": true,
	"coc.preferences.hoverTarget": "float",
	"suggest.floatEnable": true,
	"suggest.enablePreselect": false,
	"diagnostic.virtualText": false,
	"diagnostic.messageTarget": "float",
	"signature.target": "float",
	"highlight.document.enable": true,
	"highlight.colors.enable": true,
	"refactor.openCommand": "bel split"
}
