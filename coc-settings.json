{
	"diagnostic.virtualText": false,
	"list.source.commits.defaultOptions": [
		"--auto-preview"
	],
	"list.source.branches.defaultOptions": [
		"--auto-preview"
	],
	"list.source.output.defaultOptions": [
		"--auto-preview"
	],
	"list.source.buffers.defaultOptions": [
		"--auto-preview"
	],
	"list.source.maps.defaultOptions": [
		"--auto-preview"
	],
	"list.source.symbols.defaultOptions": [
		"--interactive",
		"--auto-preview"
	],
	"languageserver": {
		"rust": {
			"command": "ra_lsp_server",
			"filetypes": [
				"rust"
			],
			"rootPatterns": [
				"Cargo.toml"
			]
		}
	}
}
